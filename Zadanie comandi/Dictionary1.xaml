<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys ="clr-namespace:System;assembly=System.Core"
                    xmlns:col ="clr-namespace:System.Collections;assembly=System.Runtime" xmlns:sys1="clr-namespace:System;assembly=System.Runtime"
                    >
    <col:ArrayList x:Key="textFonts">
        <sys1:String>Arial</sys1:String>
        <sys1:String>Times New Roman</sys1:String>
        <sys1:String>Verdana</sys1:String>
    </col:ArrayList>
    <col:ArrayList x:Key="textSize">
        <sys1:String>12</sys1:String>
        <sys1:String>14</sys1:String>
        <sys1:String>16</sys1:String>
    </col:ArrayList>

</ResourceDictionary>